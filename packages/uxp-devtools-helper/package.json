{
  "name": "@adobe-fixed-uxp/uxp-devtools-helper",
  "type": "module",
  "version": "1.6.0",
  "description": "Adobe Devtools helper package used by uxp cli",
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe-uxp/devtools-cli.git",
    "directory": "packages/uxp-devtools-helper"
  },
  "keywords": [
    "adobe",
    "uxp"
  ],
  "exports": {
    ".": {
      "types": "./src/DevToolsHelper.ts",
      "import": "./src/DevToolsHelper.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/DevToolsHelper.d.ts",
        "import": "./dist/DevToolsHelper.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js"
      },
      "./package.json": "./package.json"
    }
  },
  "main": "src/DevToolsHelper.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@adobe-fixed-uxp/uxp-devtools-feature": "workspace:*",
    "node-gyp-build": "^4.2.3",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@adobe-fixed-uxp/tsconfig": "workspace:*"
  }
}
