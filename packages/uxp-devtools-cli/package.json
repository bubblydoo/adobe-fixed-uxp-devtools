{
  "name": "@adobe-fixed-uxp/uxp-devtools-cli",
  "type": "module",
  "version": "1.6.4",
  "description": "Command line interface for rapid UXP plugin development",
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe-uxp/devtools-cli.git",
    "directory": "packages/uxp-devtools-cli"
  },
  "keywords": [
    "adobe",
    "uxp",
    "cli",
    "plugin"
  ],
  "exports": {
    ".": {
      "types": "./src/uxp.ts",
      "import": "./src/uxp.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/uxp.d.ts",
        "import": "./dist/uxp.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js"
      },
      "./package.json": "./package.json"
    },
    "bin": {
      "uxp": "dist/uxp.js"
    }
  },
  "main": "src/uxp.ts",
  "bin": {
    "uxp": "src/uxp.ts"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@adobe-fixed-uxp/uxp-devtools-core": "workspace:*",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "lodash-es": "^4.17.21",
    "prompts": "^2.4.2",
    "semver": "^7.7.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@adobe-fixed-uxp/tsconfig": "workspace:*",
    "@types/ignore-walk": "^4.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@types/yargs": "^17.0.33"
  }
}
