{
  "name": "@adobe-fixed-uxp/uxp-devtools-core",
  "type": "module",
  "version": "1.6.0",
  "description": "UXP Devtools Core Module",
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe-uxp/devtools-cli.git",
    "directory": "packages/uxp-devtools-core"
  },
  "keywords": [
    "adobe",
    "uxp",
    "devtools",
    "plugin",
    "core"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js"
      },
      "./package.json": "./package.json"
    }
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@adobe-fixed-uxp/uxp-devtools-helper": "workspace:*",
    "archiver": "^7.0.1",
    "chalk": "^5.4.1",
    "chokidar": "^4.0.3",
    "express": "^4.21.2",
    "ignore-walk": "^7.0.0",
    "lodash-es": "^4.17.21",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@adobe-fixed-uxp/tsconfig": "workspace:*",
    "@types/archiver": "^6.0.3",
    "@types/express": "^5.0.0",
    "@types/ignore-walk": "^4.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/ws": "^8.5.14"
  }
}
